<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.lunatech.example.sietse.StackSearch"
          android:versionCode="1"
          android:versionName="1.0">
    <uses-sdk android:minSdkVersion="13" android:targetSdkVersion="13"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <application android:label="@string/app_name" android:icon="@drawable/ic_launcher">
        <activity android:name=".ImporterActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ListViewLoader"
            android:label="All users"/>

        <provider android:name=".UserProvider"
            android:authorities="com.lunatech.example.StackSearch.UserProvider" />
    </application>
</manifest>
